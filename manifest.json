{
  "name": "FB Talk",
  "version": "1.1",
  "description": "This app allows people to have their newsfeed read out loud",
  "app": {
    "launch": {
      "local_path": "main.html"
    }
  },  
  "icons": {
    "16": "img/cloud_logo_16.png",
    "128": "img/128fbtalk.png"
    
  },    	
  "permissions": [
    "tabs",
    "*.facebook.com/*",
    "*.fbcdn.net/*",
    "unlimitedStorage",
    "tts"
  ]
}
